{
    "parser": "@typescript-eslint/parser",
  
    "extends": [
      "prettier",
      "plugin:prettier/recommended",
      "react-app"
    ],
  
    "parserOptions": {
      "ecmaVersion": 7,
      "ecmaFeatures": {
        "modules": true,
        "jsx": true
      }
    },
  
    "plugins": [
      "@typescript-eslint"
    ],

    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "warn"
    }
  }
